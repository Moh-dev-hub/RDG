import matplotlib.pyplot as plt

data = {
   # csv donn√©es
}

labels = data.keys()
sizes = data.values()

# figure
plt.figure(figsize=(8, 8))
plt.pie(sizes, labels=labels, autopct='%1.1f%%', startangle=140, pctdistance=0.85, labeldistance=1.1)
plt.title('Distribution par champ de recherche')
plt.axis('equal')  

# affichage figure
plt.tight_layout() 
plt.show()
